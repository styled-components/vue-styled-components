{
  "name": "vue-styled-components",
  "version": "2.0.0",
  "description": "Visual primitives for the component age. A simple port of styled-components ðŸ’… for Vue",
  "main": "lib/index.js",
  "module": "dist/vue-styled-components.es.js",
  "types": "dist/index.d.ts",
  "author": "Lorenzo Girardi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/styled-components/vue-styled-components/issues"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:dist",
    "prebuild:lib": "rm -rf lib/*",
    "build:lib": "babel --out-dir lib src",
    "prebuild:umd": "rm -rf dist/*",
    "prebuild:dist": "rm -rf dist/*",
    "build:dist": "rollup -c && rollup -c --environment PRODUCTION",
    "build:watch": "npm run build:lib -- --watch",
    "format": "prettier --write \"src/**/*.js\"",
    "test:cy": "cross-env NODE_ENV='test' start-server-and-test dev http-get://localhost:3000/ \"cypress open\"",
    "test:cy:ci": "cross-env NODE_ENV='test' start-server-and-test dev http-get://localhost:3000/ \"cypress run\"",
    "test:cy:dev": "cross-env NODE_ENV='test' CYPRESS_BASE_URL=http://localhost:3000 CYPRESS_STAGE=dev start-server-and-test dev http-get://localhost:3000/ \"cypress open\"",
    "test:unit": "mocha \"./src/**/*.test.js\" --require @babel/register --require ./mocha-bootstrap --timeout 5000",
    "test:unit:dev": "npm run test -- --watch",
    "test": "npm run test:unit && npm run test:cy:ci",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prepublish": "npm run build",
    "lint-staged": "lint-staged",
    "dev": "node example/devServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/styled-components/vue-styled-components.git"
  },
  "keywords": [
    "vue",
    "css",
    "css-in-js"
  ],
  "dependencies": {
    "glamor": "^2.20.40",
    "inline-style-prefixer": "^6.0.0",
    "lodash.isplainobject": "^4.0.6",
    "lodash.zipobject": "^4.1.3",
    "stylis": "^3.5.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-external-helpers": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/preset-env": "^7.9.5",
    "@babel/register": "^7.9.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "chai": "^4.3.6",
    "chai-colors": "^1.0.1",
    "chokidar": "^3.3.1",
    "cross-env": "^7.0.3",
    "cypress": "^9.5.4",
    "danger": "^10.1.1",
    "eslint": "^8.13.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.6.0",
    "expect": "^27.5.1",
    "express": "^4.17.1",
    "jsdom": "^19.0.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.1.7",
    "lodash": "^4.17.15",
    "mocha": "^9.2.2",
    "node-watch": "^0.6.3",
    "pre-commit": "^1.2.2",
    "prettier": "^2.6.2",
    "rollup": "^2.7.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-inject": "^3.0.2",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-visualizer": "^4.0.4",
    "rollup-plugin-vue2": "^0.8.1",
    "start-server-and-test": "^1.14.0",
    "vue": "^3.0.0"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "npm run format",
      "git add"
    ]
  },
  "pre-commit": "lint-staged"
}
